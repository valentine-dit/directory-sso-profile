{
  "host_env_var_prefix": "SSO_PROFILE",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "DIRECTORY_API_CLIENT_BASE_URL",
      "DIRECTORY_API_CLIENT_KEY",
      "SECRET_KEY",
      "SSO_API_CLIENT_BASE_URL",
      "SSO_API_CLIENT_KEY",
      "SSO_LOGIN_URL",
      "SSO_LOGOUT_URL",
      "SSO_PASSWORD_RESET_URL",
      "SSO_REDIRECT_FIELD_NAME",
      "SSO_SESSION_COOKIE",
      "SSO_SIGNUP_URL",
      "UTM_COOKIE_DOMAIN"
    ],
    "optional": [
      "PORT",
      "DEBUG",
      "GOOGLE_TAG_MANAGER_ID"
    ]
  }
}
